{
  "name": "wot-server",
  "version": "0.1.0",
  "description": "WoT Server for Pi",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "bin": {
    "wot-server": "dist/index.js"
  },
  "scripts": {
    "test": "echo \"No test specified!\" && exit 1",
    "watch": "tsc -w -b src",
    "build": "tsc -b src",
    "build:w": "tsc -b src -w",
    "start": "node dist/index.js",
    "start:w": "nodemon --watch ./dist -e js dist/index.js",
    "start:dev": "yarn build && concurrently \"yarn build:w\" \"yarn start:w\"",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "coap": "^0.22.0",
    "fastify": "^2.0.0-rc.4",
    "fastify-cors": "^2.0.1",
    "fastify-plugin": "^1.4.0",
    "onoff": "^3.2.2",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/fastify-cors": "^0.1.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/onoff": "^3.2.1",
    "@types/pino": "^5.8.3",
    "@types/pino-http": "^4.0.0",
    "@types/supertest": "^2.0.7",
    "@types/ws": "^6.0.1",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "pino-pretty": "^2.5.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "superagent": "^4.1.0",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.14.1",
    "typescript": "^3.2.2",
    "vrsource-tslint-rules": "^6.0.0"
  },
  "optionalDependencies": {
    "redis": "^2.8.0"
  }
}
