{
  "name": "@baberthal/wot-server",
  "version": "0.1.0",
  "description": "WoT Server Libraries",
  "main": "dist/index.js",
  "files": [
    "README.md",
    "config",
    "dist",
    "dist-test",
    ".sequelizerc",
    "jest.config.js"
  ],
  "license": "MIT",
  "bin": {
    "wot-server": "dist/index.js"
  },
  "scripts": {
    "pretest": "yarn build",
    "test": "yarn test:unit",
    "test:w": "yarn test --watch",
    "test:unit": "NODE_ENV=test jest --config=jest.config.js --coverage",
    "test:integration": "",
    "prepackageOnly": "yarn clean && yarn build:prod",
    "prebuild": "yarn clean",
    "b": "yarn build",
    "build": "yarn build:dev",
    "build:w": "tsc -b src -w",
    "build:dev": "tsc -b src",
    "build:prod": "tsc -b src/tsconfig-build.json",
    "start": "node dist/index.js",
    "start:w": "nodemon --watch ./dist -e js dist/index.js",
    "start:dev": "yarn build && concurrently \"yarn build:w\" \"yarn start:w\"",
    "lint": "tslint -c tslint.json -p src/tsconfig.json",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "@baberthal/wot-core": "0.1.0",
    "@types/weak": "^1.0.0",
    "body-parser": "^1.18.3",
    "coap": "^0.22.0",
    "fastify": "^2.0.0-rc.4",
    "fastify-cors": "^2.0.1",
    "fastify-plugin": "^1.4.0",
    "ffi": "^2.3.0",
    "pino": "^5.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sequelize-typescript": "^0.6.7",
    "sqlite3": "^4.0.6",
    "typescript-mix": "^3.1.3",
    "weak": "^1.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/fastify-cors": "^0.1.0",
    "@types/ffi": "^0.2.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.18",
    "@types/pigpio": "^1.2.0",
    "@types/pigpio-dht": "^1.1.0",
    "@types/pino": "^5.8.3",
    "@types/pino-http": "^4.0.0",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "override-require": "^1.1.1",
    "pigpio-mock": "^0.0.1",
    "pino-caller": "^3.0.0",
    "pino-pretty": "^2.5.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "superagent": "^4.1.0",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.2",
    "typescript-json-schema": "^0.35.0",
    "vrsource-tslint-rules": "^6.0.0"
  },
  "optionalDependencies": {
    "pigpio": "^1.2.1",
    "redis": "^2.8.0"
  }
}
